<h1>Documents#new</h1>
<%= form_with model: @document, local: true do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %><br>

  <%= form.label :description %>
  <%= form.text_area :description %><br>

  <%= form.label :file %>
  <%= form.file_field :file %><br>

  <%= form.submit "Upload" %>
<% end %>

